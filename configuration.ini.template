[cvemate]
loglevel: info
save_data: False

[mongodb]
Host: 127.0.0.1
Port: 27017
DB: cvemate
Username: admin
Password: admin
AuthDB: admin
Prefix: cvemate_

[nvd]
url: https://services.nvd.nist.gov/rest/json/cves/2.0
